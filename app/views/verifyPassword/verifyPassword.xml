<Page loaded="pageLoaded">
  <ActionBar id="actionBar" title="FAB Wallet" class="action-bar" ></ActionBar> 
  <!-- Your UI components go here -->


<!--<StackLayout orientation="vertical" verticalAlignment="center"  >-->
<GridLayout id="myGrid" rows="auto">
<StackLayout id="myStack" orientation="vertical" verticalAlignment="center"  >
<Label class="h1 mh10 center" editable="false" id="title" text="Verify Your Password" textWrap="true" ></Label>

<TextField text="{{ password }}" id="password" class="mh10" returnKeyType="next" returnPress="test" secure="true" hint="Verify Password..." />

<Button class="btn btn-outline btn-rounded-lg" id="nextBtn"  text="Next " tap="test" />

<Label class="h4 mh10 center" editable="false" id="info" style=
"margin-top:50;margin-bottom:20;" text="" textWrap="true" ></Label>


</StackLayout>

</GridLayout>

</Page>
